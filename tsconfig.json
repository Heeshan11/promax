{
  "compilerOptions": {
    "moduleResolution": "node",
    "esModuleInterop": true,
    "types": ["node"],
    "target": "ES6",
    "lib": ["es6", "dom"],
    "baseUrl": ".",
    "paths": {
      "@data/*": ["src/data/*"],
      "@components/*": ["src/components/*"],
      "@app/*": ["src/*"],
      "@pages/*": ["src/pages/*"]
    },
    "jsx": "react-jsx"
  }
}
